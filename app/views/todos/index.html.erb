<h1>To do list Index</h1>

<% @todos.each do |todo| %>
<div>
	<p>
		<%= todo.item %>
		<%= link_to "Edit", edit_todo_path(todo) %>
		<%= link_to "Delete", todo, method: :delete %>
	</p>
</div>
<% end %>
<%= link_to 'Create new item', new_todo_path %></br>
<%= link_to 'Sign out', destroy_user_session_path, :method => :delete %>
